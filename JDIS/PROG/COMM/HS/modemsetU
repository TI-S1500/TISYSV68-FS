:
#modemset - UNIX version - High speed modem
# 2/2/95 - R95-2 - C.G.KOLWEY - ADD BLAST ERROR TO MODEMSET.LOG ALSO
# 12/7/95 - R961SEC - C.G.KOLWEY - REMOVE sys000
#
chmod 666 /dev/tty00
rm -f /JDIS/PROG/COMM/modemset.log
rm -f /JDIS/PROG/COMM/modemset.out
cd /JDIS/PROG/COMM
BLASTDIR=/JDIS/PROG/COMM
export BLASTDIR
SETUPDIR=/JDIS/PROG/COMM
export SETUPDIR
./blast modemset -n
if test $? != 0
then
        echo "\nmodemset: blast error while optioning the modem.\n" >>modemset.log
        exit 1
else
        echo "\nmodemset options successfully set \n" >>modemset.log
fi
exit 0
