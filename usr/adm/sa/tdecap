{%%                                    ; Start terminal definition
bpc                                    ; Business Pro console
0x1b 0x5b 0x31 0x32 0x6d 0             ; Graph on sequence
0x3a 0                                 ; Vertical line character
0x4d 0                                 ; Horizontal line character
0x49 0                                 ; Northwest corner character
0x48 0                                 ; Southwest corner character
0x3b 0                                 ; Northeast corner character
0x3c 0                                 ; Southeast corner character
0x2a 0                                 ; Block character
0x20 0                                 ; Erase character
0x4b 0                                 ; North indentation character
0x4a 0                                 ; South indentation character
0x39 0                                 ; East indentation character
0x4c 0                                 ; West indentation character
0x1b 0x5b 0x31 0x30 0x6d 0             ; Graph off sequence
0x0                                    ; Cursor on sequence
0x0                                    ; Cursor off sequence
0x0                                    ; Set terminal attribute sequence
0x1b 0x5b 0x31 0x37 0x6d 0             ; Enable reverse video
0x1b 0x5b 0x31 0x6d 0                  ; Disable reverse video
0x0                                    ; Enable underline attribute
0x0                                    ; Disable underline attribute
0x0                                    ; No display
0x0                                    ; Enable blinking attribute
0x0                                    ; Disable blinking attribute
0x1b 0x5b 0x31 0x6d 0                  ; Reset terminal attributes off
0                                      ; Set color sequence
0                                      ; Set white color
0                                      ; Set yellow color
0                                      ; Set cyan color
0                                      ; Set green color
0                                      ; Set magenta color
0                                      ; Set red color
0                                      ; Set blue color
0                                      ; Set black color
0                                      ; Reset to normal emulation
%%}                                    ; End terminal definition

