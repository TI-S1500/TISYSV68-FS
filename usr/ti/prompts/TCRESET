#	TO RESTRICTIONS SET FORTH IN TI'S PROGRAM LICENSE AGREEMENT AND
#	ASSOCIATED DOCUMENTATION.
# 
onintr end
prompt "^TCRESET -- FORCES TC DOWNLOAD "\
       "^TC Name(s):" tcnames string All\
       "^Are You Sure?:" sure yesno No\
       "^#$*"

if (! $abort) then
  if ($sure == "y") then
    if ("$tcnames" == "All") then
      /usr/bin/tcreset -R
    else
      /usr/bin/tcreset $tcnames
    endif
  endif
endif
end:
