onintr end
prompt "^TCSTAT -- DISPLAYS TC STATUS "\
       "^TC Name(s):" tcnames string All\
       "^*Output Path Name:" out acnm\
       "^#$*"

if (! $abort) then
  if ("$tcnames" == "All") then
    if ($out == "") then
      /usr/bin/tcstat
    else
      /usr/bin/tcstat > $out  
    endif
  else
    if ($out == "") then
      /usr/bin/tcstat $tcnames
    else 
      /usr/bin/tcstat $tcnames > $out
    endif
  endif
endif
end:
