onintr end
prompt "^NETEXEC -- NETWORK BATCH EXECUTION "\
       "^Remote Site Name:" nx_site string $nx_site\
       "^Executable File Path Name:" xfile string $xfile\
       "^*Listing File Path Name:" lfile string\
       "^*Execution Parameters:" parm string\
       "^#$*"

if (! $abort) then
  if ("$lfile" != "") then
    netexec -l$lfile $nx_site $xfile $parm
else
    netexec $nx_site $xfile $parm
endif
  
endif
end:
