# cshf 	MORE - DISPLAY A FILE BY SCREENS 
#	@(#)MORE.sh	1.1  87/03/09
#
onintr end

prompt  "^MORE -- INVOKES THE FILE PERSUAL FILTER FOR VDT VIEWING"\
	"^File Name(s) :" _input_file macnm "$_input_file"\
	"^#$*"


if ($abort) goto end
echo "=== MORE COMMAND EXECUTING ==="
echo more    $_input_file
     more    $_input_file

end:
