onintr end
prompt "^STRNSTAT -- DISPLAY STREAMSNET STATISTICS "\
       "^Interface Name:" ifname string All\
       "^Clear After Display:" reset yesno No\
       "^#$*"

if (! $abort) then
  if ($ifname == "All") then
    if ($reset == "n") then
      strnstat -I 
    else
      strnstat -Ic
    endif
  else
    if ($reset == "n") then
      strnstat -i $ifname
    else
      strnstat -ci $ifname
    endif
  endif

  unset ifname reset
endif
end:
